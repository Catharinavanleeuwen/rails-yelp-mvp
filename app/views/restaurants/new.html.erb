<h1>Restaurant#new</h1>
<p>Find me in app/views/restaurant/new.html.erb</p>
<%= form_for (@restaurant) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :address %>
<%= f.text_field :address %>

<%= f.label :phone_number %>
<%= f.text_field :phone_number %>


<%= f.label :category %>

<%= f.select :category, @category %>
<%= f.submit "add restaurant", class: "btn btn-primary" %>
<% end %>
